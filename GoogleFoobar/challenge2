# l is the input list of unsorted strings

def solution(l):
    string = ""
    output = []
    fin_output = []
    for i in l:
        m = i.split(".")
        for j in range(0, len(m)):
            m[j] = int(m[j])
        output.append(m)
    for j in sorted(output):
        for k in j:
            string = string + str(k) + "."
        fin_output.append(string[:-1])
        string = ""
    return(fin_output)
